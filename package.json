{
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.17.1",
    "@yaireo/tagify": "^4.8.1",
    "antd": "^4.10.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "grapesjs": "^0.17.26",
    "grapesjs-blocks-basic": "^0.1.8",
    "immutable": "^4.0.0-rc.14",
    "js-cookie": "^2.2.1",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^5.0.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.10.16",
    "react-query": "^3.6.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "start": "copy .env.local .env && set PORT=3001 && react-scripts start",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install-core": "yarn add antd axios js-cookie node-sass@4.14.1 nprogress react react-dom react-intl react-router-dom react-scripts react-query classnames"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
