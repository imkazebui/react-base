{
  "name": "react-base",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^4.10.3",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "node-sass": "4.14.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intl": "^5.10.16",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "redux-devtools-extension": "^2.13.8"
  },
  "scripts": {
    "start": "cp .env.local .env && PORT=3001 react-scripts start",
    "build": "sh -ac '. .env.${REACT_APP_ENV}; react-scripts build'",
    "build:dev": "REACT_APP_ENV=dev npm run build",
    "build:staging": "REACT_APP_ENV=staging npm run build",
    "build:prod": "REACT_APP_ENV=prod npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install-core": "yarn add antd axios js-cookie node-sass@4.14.1 nprogress react react-dom react-intl react-router-dom react-scripts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
